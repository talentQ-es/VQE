	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/qua.26352},
	keywords = {excited states, multireference states, quantum computation, spin symmetry, unitary coupled cluster, Variational Quantum Eigensolver, VQE algorithm},
	pages = {e26352},
	file = {Full Text PDF:/home/jscqcom/snap/zotero-snap/common/Zotero/storage/KWWFGMF2/Greene-Diniz y Muñoz Ramo - 2021 - Generalized unitary coupled cluster excitations fo.pdf:application/pdf;Snapshot:/home/jscqcom/snap/zotero-snap/common/Zotero/storage/B3XR4S8F/qua.html:text/html},
